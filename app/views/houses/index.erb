<h1>All Houses</h1>

<ul>
  <% House.all.each do |house| %>
    <li><a href="/houses/<%= house.id %>"><%= house.address %></a></li>
    <ul>
      <li> Unclaimed Candy: </li>
      <% house.unclaimed_candies.each do |candy| %>
        <li> <%= candy.name %> </li>
      <% end %>
    </ul>
  <% end %>
</ul>